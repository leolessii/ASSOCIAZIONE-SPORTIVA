<Window x:Class="WpfAssociazione.HomeAssociazione"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAssociazione"
        mc:Ignorable="d"
        Title="HomeAssociazione" >
    <Grid Background="#FF232323" Height="1080" Width="1920">
        <Rectangle HorizontalAlignment="Left" Margin="871,356,0,40" Stroke="Black" Width="301" Fill="#FF757575"/>
        <Rectangle Margin="871,103,0,0" Stroke="Black" Fill="#FF757575" Height="165" VerticalAlignment="Top" HorizontalAlignment="Left" Width="304"/>
        <Rectangle Margin="1236,103,0,0" Stroke="Black" Fill="#FF757575" Height="570" VerticalAlignment="Top" HorizontalAlignment="Left" Width="335"/>
        <Label x:Name="lbl4" Content="Atleta" Margin="32,28,0,0" FontSize="36" BorderBrush="Black" BorderThickness="3,3,3,3" Background="#FFDC8301" HorizontalContentAlignment="Center" FontFamily="Bahnschrift SemiBold" HorizontalAlignment="Left" Width="234" Height="70" VerticalAlignment="Top"/>
        <ListBox x:Name="lstAtleti" Margin="32,98,0,40" BorderBrush="Black" BorderThickness="3,3,3,3" FontSize="24" Background="#FF757575" HorizontalAlignment="Left" Width="234" SelectionChanged="lstAtleti_SelectionChanged" />
        <Label x:Name="lbl4_Copia" Content="Istruttore" Margin="303,28,0,0" FontSize="36" BorderBrush="Black" BorderThickness="3,3,3,3" Background="#FFDC8301" FontFamily="Bahnschrift SemiBold" HorizontalContentAlignment="Center" HorizontalAlignment="Left" Width="236" Height="70" VerticalAlignment="Top"/>
        <ListBox x:Name="lstIstruttori" Margin="303,98,0,40" BorderBrush="Black" HorizontalAlignment="Left" Width="236" BorderThickness="3,3,3,3" FontSize="24" Background="#FF757575" SelectionChanged="lstIstruttori_SelectionChanged"/>
        <Label x:Name="lbl4_Copia1" Content="Amministratori" Margin="598,28,0,0" FontSize="36" BorderBrush="Black" BorderThickness="3,3,3,3" Background="#FFDC8301" HorizontalContentAlignment="Center" FontFamily="Bahnschrift SemiBold" HorizontalAlignment="Left" Width="215" Height="70" VerticalAlignment="Top"/>
        <ListBox x:Name="lstAmministratori" Margin="598,98,0,40" BorderBrush="Black" BorderThickness="3,3,3,3" FontSize="24" Background="#FF757575" HorizontalAlignment="Left" Width="215" SelectionChanged="lstAmministratori_SelectionChanged"/>
        <Button x:Name="btn_AggiungiTesserato" Content="Aggiungi Tesserato" Margin="1244,130,0,0" BorderBrush="Black" FontSize="36" BorderThickness="3,3,3,3" Background="#FFDC8301" FontFamily="Bahnschrift SemiBold" HorizontalAlignment="Left" Width="319" Height="108" VerticalAlignment="Top" Click="btn_AggiungiTesserato_Click"/>
        <Button x:Name="btn_RimuoviTesserato" Content="Rimuovi Tesserato" Margin="1244,243,0,0" BorderBrush="Black" FontFamily="Bahnschrift SemiBold" FontSize="36" BorderThickness="3,3,3,3" Background="#FFDC8301" HorizontalAlignment="Left" Width="319" Height="108" VerticalAlignment="Top" Click="btn_RimuoviTesserato_Click"/>
        <Button x:Name="btn_AggiungiCertificato" Content="Aggiungi Certificato" Margin="1244,356,0,0" BorderBrush="Black" FontSize="36" BorderThickness="3,3,3,3" Background="#FFDC8301" FontFamily="Bahnschrift SemiBold" HorizontalAlignment="Left" Width="319" Height="108" VerticalAlignment="Top" Click="btn_AggiungiCertificato_Click"/>
        <Label x:Name="lbl4_Copia2" Content="Tesserati" Margin="1236,28,0,0" FontSize="36" BorderBrush="Black" BorderThickness="3,3,3,3" Background="#FFDC8301" HorizontalContentAlignment="Center" FontFamily="Bahnschrift SemiBold" HorizontalAlignment="Left" Width="335" Height="70" VerticalAlignment="Top"/>
        <Label x:Name="lbl4_Copia3" Content="Associazione" Margin="870,28,0,0" FontSize="36" BorderBrush="Black" BorderThickness="3,3,3,3" Background="#FFDC8301" HorizontalContentAlignment="Center" FontFamily="Bahnschrift SemiBold" HorizontalAlignment="Left" Width="304" Height="70" VerticalAlignment="Top"/>
        <Button x:Name="btn_ModificaSpecialiàTesserato" Content="Modifica Specialità" Margin="1244,475,0,0" BorderBrush="Black" FontSize="36" BorderThickness="3,3,3,3" Background="#FFDC8301" FontFamily="Bahnschrift SemiBold" HorizontalAlignment="Left" Width="319" Height="108" VerticalAlignment="Top" Click="btn_ModificaSpecialiàTesserato_Click"/>
        <Label x:Name="lblInfo" Content="Info Tesserato" Margin="871,281,0,0" FontSize="36" BorderBrush="Black" BorderThickness="3,3,3,3" Background="#FFDC8301" HorizontalContentAlignment="Center" FontFamily="Bahnschrift SemiBold" HorizontalAlignment="Left" Width="304" Height="70" VerticalAlignment="Top"/>
        <Label x:Name="lbl5" Content="" Margin="878,376,0,225" FontSize="24" BorderThickness="3,3,3,3" Background="{x:Null}" HorizontalAlignment="Left" Width="287"/>
        <Button x:Name="btn_ModificaSpecialiàAssociazione" Content="Modifica Specialità" Margin="879,130,0,0" BorderBrush="Black" FontSize="36" BorderThickness="3,3,3,3" Background="#FFDC8301" FontFamily="Bahnschrift SemiBold" HorizontalAlignment="Left" Width="289" Height="108" VerticalAlignment="Top" Click="btn_ModificaSpecialiàAssociazione_Click"/>

    </Grid>
</Window>
